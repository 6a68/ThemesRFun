{
  "name": "themesrfun",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel server watch:extension",
    "server": "NODE_ENV=development webpack-dev-server --config webpack.web.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:web": "NODE_ENV=development webpack --watch --progress --colors --config webpack.web.js",
    "watch:extension": "NODE_ENV=development webpack --watch --progress --colors --config webpack.extension.js",
    "build": "npm-run-all --parallel build:*",
    "build:web": "NODE_ENV=production webpack -p --config webpack.web.js",
    "build:extension": "NODE_ENV=production webpack -p --config webpack.extension.js",
    "clean": "rm -rf addon.xpi build web-ext-artifacts",
    "package": "npm run clean && npm run build:extension && web-ext build --overwrite-dest --source-dir=build/extension && mv web-ext-artifacts/*.zip ./addon.xpi",
    "run": "npm run build && web-ext run --artifacts-dir=. --source-dir=build/extension",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "author": "",
  "license": "MPL2",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-node8": "^1.2.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "ejs": "^2.5.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "npm-run-all": "^4.1.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "svg-url-loader": "^2.3.0",
    "url-loader": "^0.6.2",
    "web-ext": "^2.2.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1",
    "write-file-webpack-plugin": "^4.2.0"
  }
}
